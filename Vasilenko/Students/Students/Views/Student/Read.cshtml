@model IEnumerable<Students.Models.Student>

@{
    ViewBag.Title = "Read";
}

<h2>Read</h2>

<table width="750" cellpadding="5" cellspacing="0">
    <tr>
        <td width="150"> ID </td>
        <td width="400">FirstName</td>
        <td width="200">LastName</td>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td width="150">@item.ID </td>
            <td width="400">@item.FirstName</td>
            <td width="200">@item.LastName</td>
        </tr>
    }
</table>

<h2>Delete Student</h2>
@using Data.Contracts.Entities;
@model List<Student>

@using (Html.BeginForm("Delete", "Student", FormMethod.Post))
{
    <h3>Enter id for delete</h3>
    @Html.Label("id")
    @Html.Editor("id")
    <input type="submit" value="delete" />
}
<br />
<hr />
<br />
@{
    Html.RenderPartial("ListOfStudentsPartial");
}